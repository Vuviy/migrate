#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

use App\Console\MyCommand;
use App\Console\MyCommand2;
use Symfony\Component\Console\Application;

$app = new Application('My CLI');

$app->addCommands(
        [
            new MyCommand(),
            new MyCommand2()
        ]);

$app->run();